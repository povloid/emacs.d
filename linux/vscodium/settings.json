{
    "editor.fontFamily": "PT Mono Bold",
    "workbench.iconTheme": "vscode-great-icons",
    "editor.fontWeight": "600",
    "editor.fontSize": 13,
    "editor.lineHeight": 0,
    "editor.lineNumbers": "off",
    "editor.renderLineHighlight": "none",
    "editor.showFoldingControls": "never",
    "editor.guides.indentation": false,
    "editor.matchBrackets": "never",
    "terminal.integrated.fontWeightBold": "bold",
    "terminal.integrated.lineHeight": 1,
    "terminal.integrated.fontWeight": "bold",
    "editor.fontVariations": false,
    "editor.fontLigatures": false,
    "editor.mouseWheelZoom": false,
    "terminal.integrated.sendKeybindingsToShell": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "always",
        "source.organizeImports": "always",
        "source.sortMembers": "never"
    },
    "explorer.compactFolders": false,
    "redhat.telemetry.enabled": false,
    "java.jdt.ls.java.home": "/opt/java/jdk",
    "redHatDependencyAnalytics.mvn.executable.path": "/opt/java/apache-maven/bin/mvn",
    "workbench.colorTheme": "Default Dark+",
    "git.confirmSync": false,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontFamily": "monospace",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "workbench.editor.showTabs": "single",
    "editor.cursorStyle": "underline",
    "redHatDependencyAnalytics.npm.executable.path": "/opt/node/bin/npm",
    "java.format.settings.profile": "GoogleStyle",
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml"
}