{
    "editor.fontFamily": "Iosevka",
    "editor.fontSize": 14,
    "editor.fontWeight": "500",
    "editor.lineHeight": 0,
    "workbench.iconTheme": "vscode-great-icons",
    "editor.lineNumbers": "off",
    "editor.renderLineHighlight": "none",
    "editor.showFoldingControls": "never",
    "editor.guides.indentation": false,
    "editor.matchBrackets": "never",
    "terminal.integrated.fontWeightBold": "bold",
    "terminal.integrated.lineHeight": 1,
    "terminal.integrated.fontWeight": "bold",
    "editor.fontVariations": false,
    "editor.fontLigatures": false,
    "editor.mouseWheelZoom": false,
    "terminal.integrated.sendKeybindingsToShell": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "always",
        "source.organizeImports": "always",
        "source.sortMembers": "never"
    },
    "explorer.compactFolders": false,
    "redhat.telemetry.enabled": false,
    "java.jdt.ls.java.home": "/opt/java/jdk",
    "redHatDependencyAnalytics.mvn.executable.path": "/opt/java/apache-maven/bin/mvn",
    "git.confirmSync": false,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontFamily": "Iosevka",
    "window.zoomLevel": 0.5,
    "workbench.editor.showTabs": "none",
    "editor.cursorStyle": "underline",
    "redHatDependencyAnalytics.npm.executable.path": "/opt/node/bin/npm",
    "editor.minimap.showSlider": "always",
    "workbench.editor.tabActionCloseVisibility": false,
    "window.confirmBeforeClose": "always",
    "workbench.editor.editorActionsLocation": "titleBar",
    "git.autofetch": true,
    "editor.minimap.enabled": false,
    "debug.console.fontFamily": "Iosevka",
    "debug.console.fontSize": 13,
    "editor.stickyScroll.enabled": false,
    "workbench.tree.enableStickyScroll": false,
    "editor.inlayHints.enabled": "off",
    "extensions.autoUpdate": false,
    "editor.detectIndentation": false,
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "just-eclipse-color-theme"
}