{
    "editor.fontFamily": "Pt Mono",
    "editor.fontLigatures": true,
    "editor.fontWeight": "600",
    "editor.lineHeight": 0,
    "editor.lineNumbers": "off",
    "editor.renderLineHighlight": "none",
    "editor.showFoldingControls": "never",
    "editor.guides.indentation": false,
    "editor.matchBrackets": "never",
    "terminal.integrated.fontWeightBold": "bold",
    "terminal.integrated.lineHeight": 1,
    "terminal.integrated.fontWeight": "bold",
    "editor.fontVariations": false,
    "editor.mouseWheelZoom": false,
    "terminal.integrated.sendKeybindingsToShell": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "always",
        "source.organizeImports": "always",
        "source.addMissingImports": "always",
        "source.sortMembers": "never"
    },
    "explorer.compactFolders": false,
    "git.confirmSync": false,
    "terminal.integrated.fontSize": 11,
    "window.zoomLevel": 0.5,
    "editor.minimap.showSlider": "always",
    "workbench.editor.tabActionCloseVisibility": false,
    "window.confirmBeforeClose": "always",
    "workbench.editor.editorActionsLocation": "hidden",
    "git.autofetch": true,
    "debug.console.fontFamily": "Roboto Mono",
    "debug.console.fontSize": 12,
    "editor.stickyScroll.enabled": false,
    "workbench.tree.enableStickyScroll": false,
    "editor.inlayHints.enabled": "off",
    "extensions.autoUpdate": false,
    "editor.detectIndentation": false,
    "window.titleBarStyle": "custom",
    "sonarlint.rules": {
        "java:S107": {
            "level": "on"
        }
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "files.simpleDialog.enable": true,
    "window.title": "${rootName}",
    "sonarlint.pathToNodeExecutable": "/usr/bin/node",
    "workbench.startupEditor": "none",
    "window.customTitleBarVisibility": "auto",
    "window.experimentalControlOverlay": false,
    "chat.commandCenter.enabled": false,
    "workbench.navigationControl.enabled": false,
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "workbench.tree.indent": 18,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.expandMode": "doubleClick",
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "workbench.preferredHighContrastColorTheme": "Default Light+",
    "terminal.integrated.fontFamily": "PT Mono",
    "workbench.iconTheme": "vscode-icons",
    "javascript.preferences.organizeImports": {},
    "http.proxy": "socks5://localhost:35353",
    "http.proxyStrictSSL": false,
    "workbench.colorTheme": "Visual Studio Light"
}
